<div class="banner">
  <div class="container">
    <h1>Save any kind of movies</h1>
    <p>
      <span id="banner-typed-text"></span>
    </p>
  </div>
</div>

<div class="container">
  <div class="list-text">
    <h3>My Lists</h3>
    <%= link_to 'Create a new list', new_list_path(@list), class: "btn btn-success" %>
  </div>

  <div class="lists">
    <% @lists.each do |list| %>
      <%= render '/lists/list', list: list %>
    <% end %>
  </div>


</div>
